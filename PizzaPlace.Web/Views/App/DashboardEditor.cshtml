<div class="modal-content">
    <form novalidate name="vm.moduleForm" class="form-vertical" ng-submit="vm.save()">
        <div class="modal-header">
            <button type="button" class="close" ng-click="vm.cancel()" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
            <h4 class="modal-title">{{ vm.headerTitle }}</h4>
        </div>
        <div class="modal-body">
            <div class="row center-block">
                <div class="col-md-12">
                    <div class="form-vertical">
                        <div class="form-group" ng-show="vm.showMessage">
                            <div class="alert alert-danger">
                                <span ng-repeat="message in vm.errorMessages">
                                    {{ message }}<br />
                                </span>
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="order_details_id" class="control-label">order_details_id *</label>
                            <input class="form-control"
                                   name="order_details_id"
                                   type="text"
                                   placeholder="order_details_id"
                                   required
                                   disabled
                                   ng-model="vm.dashboard.fileDetailId" />
                        </div>
                        <div class="form-group">
                            <label for="order_id" class="control-label">order_id *</label>
                            <input class="form-control"
                                   name="order_id"
                                   type="text"
                                   placeholder="order_id"
                                   required
                                   disabled
                                   ng-model="vm.dashboard.order.fileOrderId" />
                        </div>
                        <div class="form-group">
                            <label for="order_date" class="control-label">order_date *</label>
                            <input class="form-control"
                                   name="order_date"
                                   type="text"
                                   placeholder="order_date"
                                   required
                                   disabled
                                   ng-model="vm.dashboard.order.createdDate" />
                        </div>
                        <div class="form-group">
                            <label for="pizza_id" class="control-label">pizza_id *</label>
                            <input class="form-control"
                                   name="pizza_id"
                                   type="text"
                                   placeholder="pizza_id"
                                   required
                                   disabled
                                   ng-model="vm.dashboard.pizza.code" />
                        </div>
                        <div class="form-group">
                            <label for="pizza_type_id" class="control-label">pizza_type_id *</label>
                            <input class="form-control"
                                   name="pizza_type_id"
                                   type="text"
                                   placeholder="pizza_type_id"
                                   required
                                   disabled
                                   ng-model="vm.dashboard.pizza.pizzaType.code" />
                        </div>
                        <div class="form-group">
                            <label for="size" class="control-label">size *</label>
                            <input class="form-control"
                                   name="size"
                                   type="text"
                                   placeholder="size"
                                   required
                                   disabled
                                   ng-model="vm.dashboard.pizza.size" />
                        </div>
                        <div class="form-group">
                            <label for="price" class="control-label">price *</label>
                            <input class="form-control"
                                   name="price"
                                   type="text"
                                   placeholder="price"
                                   required
                                   disabled
                                   ng-model="vm.dashboard.pizza.price" />
                        </div>
                        <div class="form-group">
                            <label for="name" class="control-label">name *</label>
                            <input class="form-control"
                                   name="name"
                                   type="text"
                                   placeholder="name"
                                   required
                                   disabled
                                   ng-model="vm.dashboard.pizza.pizzaType.name" />
                        </div>
                        <div class="form-group">
                            <label for="category" class="control-label">category *</label>
                            <input class="form-control"
                                   name="category"
                                   type="text"
                                   placeholder="category"
                                   required
                                   disabled
                                   ng-model="vm.dashboard.pizza.pizzaType.category.name" />
                        </div>
                        <div class="form-group">
                            <label for="ingredients" class="control-label">ingredients *</label>
                            <input class="form-control"
                                   name="ingredients"
                                   type="text"
                                   placeholder="ingredients"
                                   required
                                   disabled
                                   ng-model="vm.dashboard.pizza.pizzaType.ingredients" />
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="modal-footer">
            <button type="button" ng-click="vm.cancel()" class="btn btn-default" data-dismiss="modal">Cancel</button>
        </div>
    </form>
</div>